Attribute VB_Name = "Programas"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Dim CopiarPrograma As Byte
Private Sub Command1_Click(Index As Integer)
ProgramaActual = Index + 1
PasoActual = 1
Editor.VScroll1 = PasoActual
Editor.VScroll2.Value = UltimoPaso(ProgramaActual)
Editor.HScroll1.Value = TiempoPaso(ProgramaActual, PasoActual)
If PlayStop = False Then
    PlayStop = True
End If
TiempoPasoAcual = 0
ActualizarEditor
ActualizarConsola
Masters.Cls
End Sub

Private Sub Command3_Click()

End Sub

Private Sub Command1_KeyPress(Index As Integer, KeyAscii As Integer)

If ModoProgramador Then
    If KeyAscii = 44 Then 'Copiar
        PlayStop = False
        TiempoPasoAcual = 0
        PasoActual = 1
        Editor.VScroll1 = 1
        CopiarPrograma = Index + 1
    End If
    If KeyAscii = 46 Then 'Pegar
        Dim AAA As Integer, BBB As Integer, CCC As Integer
        PlayStop = False
        TiempoPasoAcual = 0
        PasoActual = 1
        Editor.VScroll1 = 1
        If MsgBox("Desea pegar el programa " & EtiquetaPrograma(CopiarPrograma), vbOKCancel, Copiar) = vbOK Then
            'Pego el Programa Copiado
            EtiquetaPrograma(Index + 1) = EtiquetaPrograma(CopiarPrograma)
            UltimoPaso(Index + 1) = UltimoPaso(CopiarPrograma)
            For BBB = 1 To CantPasosProgramas
                TiempoPaso(Index + 1, BBB) = TiempoPaso(CopiarPrograma, BBB)
                For CCC = 1 To CantCanalesDMX
                    MartizDMX(Index + 1, BBB, CCC) = MartizDMX(CopiarPrograma, BBB, CCC)
                Next CCC
            Next BBB
            ActualizarProgramas
        End If
    End If
End If
End Sub

Private Sub Form_KeyPress(KeyAscii As Integer)
    Masters.Label10.Caption = KeyAscii
