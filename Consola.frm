Private Sub Form_Load()
For I = 0 To CantPotes - 1
    TT2(I).Style = TTBalloon
    TT2(I).Icon = TTIconInfo
    TT2(I).ForeColor = vbWhite
    TT2(I).BackColor = &HFF0000
    TT2(I).DelayTime = 100 '//show tooltip after 100 miliseconds
    TT2(I).PopupOnDemand = False
    TT2(I).VisibleTime = 6000   'After 6 Seconds tooltip will go away
    '//Tooltip font setting
    Dim f As New StdFont
    f.Name = "Verdana"
    f.Underline = True
    TT2(I).TipFont = f
    SSTabSubclass Consola.Slider1(I).hwnd
Next I
  
ActualizarConsola

End Sub

Private Sub Slider1_MouseUp(Index As Integer, Button As Integer, Shift As Integer, X As Single, Y As Single)
    If Button = 2 Then
        If FondoPote(Slider2.Value + Index) = 9 Then
            FondoPote(Slider2.Value + Index) = 1
        Else
            FondoPote(Slider2.Value + Index) = FondoPote(Slider2.Value + Index) + 1

        End If
    End If
    ActualizarConsola
End Sub

